{
  "name": "webpack-build",
  "version": "0.0.1",
  "description": "webpack build analyze demo",
  "main": "index.js",
  "private": true,
  "scripts": {
    "start": "cd config && webpack-dev-server",
    "build": "rm -rf dist && webpack --config config/webpack.config.js",
    "build:analyze": "rm -rf dist && BUILD_ANALYZE=true webpack --config config/webpack.config.js",
    "build:time": "rm -rf dist && TIME_ANALYZE=true webpack --config config/webpack.config.js",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Craster",
  "license": "ISC",
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "autodll-webpack-plugin": "^0.4.2",
    "babel-loader": "^8.1.0",
    "css-loader": "^3.6.0",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "less-loader": "^6.2.0",
    "prettier": "^2.0.5",
    "speed-measure-webpack-plugin": "^1.3.3",
    "terser-webpack-plugin": "^3.0.7",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  },
  "dependencies": {
    "element-ui": "^2.13.2",
    "vue": "^2.6.11",
    "vue-loader": "^15.9.3",
    "vue-router": "^3.3.4",
    "vue-template-compiler": "^2.6.11"
  }
}
